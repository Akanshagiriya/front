{
  "name": "minds",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "preinstall": "git config core.hooksPath .git/hooks/",
    "build": "ng build --prod",
    "prebuild:dev": "gulp build.sass --deploy-url=/",
    "build:dev": "ng build --watch=true --poll=1000",
    "preserve:dev": "gulp build.sass --deploy-url=http://localhost:4200/",
    "serve:dev": "ng serve --deploy-url=http://localhost:4200/ --watch=true --poll=1000 --progress --proxy-config proxy.conf.js --host=0.0.0.0 --disableHostCheck=true",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "cypress run --debug",
    "e2e-open": "cypress open",
    "postinstall": "node patch.js",
    "serve:ssr": "node dist/server/main",
    "build:ssr": "ng run minds:server:production",
    "build:ssr:dev": "ng run minds:server:production",
    "bundle-report": "webpack-bundle-analyzer dist/browser/stats.json",
    "xi18n": "gulp extract.i18n",
    "xi18n-import": "gulp import.i18n"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.4",
    "@angular/cdk": "~9.2.4",
    "@angular/common": "~10.0.4",
    "@angular/compiler": "~10.0.4",
    "@angular/core": "~10.0.4",
    "@angular/forms": "~10.0.4",
    "@angular/localize": "^10.0.4",
    "@angular/platform-browser": "~10.0.4",
    "@angular/platform-browser-dynamic": "~10.0.4",
    "@angular/platform-server": "~10.0.4",
    "@angular/router": "~10.0.4",
    "@danielmoncada/angular-datetime-picker": "^9.3.1",
    "@gorniv/ngx-universal": "^2.2.2",
    "@nguniversal/builders": "~9.1.1",
    "@nguniversal/common": "~9.1.1",
    "@nguniversal/express-engine": "~9.1.1",
    "@nguniversal/socket-engine": "~9.1.1",
    "@sentry/browser": "^5.11.1",
    "angular-plotly.js": "^2.0.0",
    "bn.js": "^4.11.8",
    "braintree-web": "3.41.0",
    "cookie-parser": "^1.4.4",
    "core-js": "~2.6.2",
    "dexie": "^2.0.4",
    "ethjs": "~0.4.0",
    "ethjs-account": "^0.1.4",
    "ethjs-provider-signer": "^0.1.4",
    "ethjs-signer": "0.1.1",
    "express-http-proxy": "^1.6.0",
    "global": "^4.3.2",
    "macy": "^2.5.1",
    "material-design-icons": "~3.0.1",
    "material-design-lite": "~1.3.0",
    "medium-editor": "^5.23.2",
    "moment": "^2.24.0",
    "ngx-drag-drop": "^2.0.0",
    "ngx-image-cropper": "^3.1.6",
    "ngx-plyr": "~4.0.0",
    "node-cache": "^5.1.0",
    "plotly.js": "^1.54.1",
    "plyr": "^3.5.6",
    "qrcodejs2": "0.0.2",
    "rxjs": "~6.6.0",
    "socket.io-client": "^2.2.0",
    "textarea-caret": "^3.1.0",
    "tslib": "^2.0.0",
    "webpack-node-externals": "^1.7.2",
    "webtorrent": "^0.103.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1000.3",
    "@angular/cli": "^10.0.3",
    "@angular/compiler-cli": "~10.0.4",
    "@angular/language-service": "~10.0.4",
    "@angularclass/hmr": "^2.1.3",
    "@locl/cli": "0.0.1-beta.9",
    "@types/express-serve-static-core": "^4.17.7",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.4",
    "@types/node": "~14.0.1",
    "cross-env": "^7.0.2",
    "cypress": "^4.3.0",
    "cypress-file-upload": "^3.3.3",
    "gulp": "~4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-css-globbing": "^0.2.2",
    "gulp-minify-css": "^1.1.6",
    "gulp-sass": "^4.0.2",
    "gulp-template": "^5.0.0",
    "husky": "^3.0.4",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jasmine-ts-async": "^1.0.0",
    "jsonwebtoken": "8.5.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-mocha-reporter": "^2.2.5",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.11.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.10.1",
    "tslint": "~6.1.0",
    "typescript": "~3.9.6",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "xmlbuilder2": "^2.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sh .githooks/pre-commit && pretty-quick --staged --bail --pattern \"**/*.*(ts|html|scss)\""
    }
  }
}
